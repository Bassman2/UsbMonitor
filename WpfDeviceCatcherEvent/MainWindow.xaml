<UsbMonitorWindow x:Class="DeviceCatcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeviceCatcher"
        mc:Ignorable="d"
        Title="Device Catcher" Height="600" Width="800" Icon="UnknownDrive.ico"
        UsbOem="UsbMonitorWindow_Usb"
        UsbVolume="UsbMonitorWindow_Usb"
        UsbPort="UsbMonitorWindow_Usb"
        UsbDeviceInterface="UsbMonitorWindow_Usb"
        UsbHandle="UsbMonitorWindow_Usb"
        UsbChanged="UsbMonitorWindow_Usb">
    <Grid>
        <TextBox x:Name="textBox" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" />
    </Grid>
</UsbMonitorWindow>
